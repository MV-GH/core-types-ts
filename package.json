{
  "name": "core-types-ts",
  "version": "0.0.0-development",
  "description": "core-types ⬌ TypeScript interface conversion",
  "author": "Gustaf Räntilä",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grantila/core-types-ts/issues"
  },
  "homepage": "https://github.com/grantila/core-types-ts#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {},
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/tsc -p tsconfig.prod.json",
    "test": "node_modules/.bin/jest --coverage",
    "cz": "git-cz --disable-emoji"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grantila/core-types-ts"
  },
  "keywords": [
    "type",
    "types",
    "generic",
    "typescript",
    "json"
  ],
  "dependencies": {
    "core-types": "^1.10.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "git-cz": "^4.9.0",
    "jest": "^29.4.1",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__snapshots__/"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "packageManager": "yarn@3.2.4"
}
